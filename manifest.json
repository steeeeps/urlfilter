{
	"manifest_version": 2,
	"name": "URL Filter",
	"version": "1.1.0",
	"background": {
		"scripts": ["js/background.js"]
	},
	"permissions": ["tabs"],
	"browser_action": {
		"default_icon": "images/t_16.png",
		"default_title": "URL Filter",
		"default_popup": "popup.html"
	},
	"options_page": "setting.html"
}
